{
  "manifest_version": 2,
  "name": "ヨドバシカメラ 検索エンジン",
  "version": "1.0.0",
  "description": "検索エンジンに「ヨドバシカメラ」を追加します。",
  "icons": {
    "48": "icon.png"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "Amazon",
