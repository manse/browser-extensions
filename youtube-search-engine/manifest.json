{
  "manifest_version": 2,
  "name": "YouTube Search Engine",
  "version": "1.0.1",
  "description": "Add 'YouTube' to the search engines.",
  "icons": {
    "48": "icon.png"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "YouTube",
      "search_url": "https://www.youtube.com/results?search_query={searchTerms}",
      "favicon_url": "https://www.youtube.com/favicon.ico",
      "keyword": "yt",
      "encoding": "UTF-8",
      "is_default": false
    }
  }
}